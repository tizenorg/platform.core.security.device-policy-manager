				align: 0.0 0.5;
			}
		}
		part { name: "locktype_button_content";
			type: SWALLOW;
			scale: 1;
			description { state: "default" 0 0;
				rel1 { relative: 0.4 0.6; to: "locktype_text";}
				rel2 { relative: 1.0 0.6; to: "locktype_text"; offset: 0 100;}
